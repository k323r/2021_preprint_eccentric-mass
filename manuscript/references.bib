@article{zendehbadMeasurementsTowerDeflections2017a,
  title = {Measurements of Tower Deflections on Full-Scale Wind Turbines Using an Opto-Mechanical Platform},
  author = {Zendehbad, M. and Chokani, N. and Abhari, R.S.},
  year = {2017},
  month = sep,
  volume = {168},
