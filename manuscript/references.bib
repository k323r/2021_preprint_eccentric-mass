@article{zhaoIntegratedDynamicAnalysis2018,
  ids = {zhaoIntegratedDynamicAnalysis2018a},
  title = {An Integrated Dynamic Analysis Method for Simulating Installation of Single Blades for Wind Turbines},
  author = {Zhao, Yuna and Cheng, Zhengshun and Sandvik, Peter Christian and Gao, Zhen and Moan, Torgeir},
  year = {2018},
  month = mar,
  volume = {152},
  pages = {72--88},
  issn = {0029-8018},
  doi = {10.1016/j.oceaneng.2018.01.046},
  abstract = {Installation of blades for wind turbines is challenging due to large lifting height and high precision. Assessment of blade dynamic responses during installation needs advanced simulation tools which are limited at present. This paper alms at developing an integrated simulation tool SIMO-Aero for single blade installation for both onshore and offshore wind turbines. Based on the cross-flow principle, the aerodynamic model is established by accounting for the effect of wind turbulence and dynamic stall. Then it is coupled with SIMO to achieve the integrated simulation tool SIMO-Aero which can account for blade aerodynamics, vessel hydrodynamics and system mechanical couplings. The aerodynamic code is verified by code-to-code comparisons with HAWC2. Furthermore, SIMO-Aero is applied in case studies on the wind-induced dynamic responses of a DTU 10 MW blade during installation using a jack-up crane vessel which is assumed to be rigid, including the crane, and rigidly fixed to the seabed. The characteristics of system dynamic responses prior to mating the blade onto the hub are studied. It is shown that the blade motions are dominated by the pendulum motion. Critical parameters of the installation process are identified. The extreme responses of critical parameters are further studied under turbulent winds and wind gusts.},
  annotation = {WOS:000428489800008},
  file = {/home/san/Zotero/storage/MNGD76CL/2018_Zhao et al_An integrated dynamic analysis method for simulating installation of single.pdf;/home/san/Zotero/storage/NA56FYAN/zhao_et_al_2018_an_integrated_dynamic_analysis_method_for_simulating_installation_of_single.pdf;/home/san/Zotero/storage/GLNA26DR/S0029801818300465.html},
  journal = {Ocean Engineering},
  keywords = {_tablet,Blade installation,Extreme response,Fully coupled method,hoisting forces,Wind turbine},
  language = {English}
}

