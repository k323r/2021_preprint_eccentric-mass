@article{feyzollahzadehWindLoadResponse2016a,
  title = {Wind Load Response of Offshore Wind Turbine Towers with Fixed Monopile Platform},
  author = {Feyzollahzadeh, M. and Mahmoodi, M.J. and {Yadavar-Nikravesh}, S.M. and Jamali, J.},
  year = {2016},
