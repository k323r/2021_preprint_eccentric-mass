  title = {{{Monitoring of Offshore Wind Turbines under Wave and Wind Loading during Installation}}},
  booktitle = {{{XI International Conference}} on {{Structural Dynamics}}},
  author = {Sander, Aljoscha and Meinhardt, Christian and Thoben, Klaus-Dieter},
