@article{obrienHorizontalAxisWind2017,
  title = {Horizontal Axis Wind Turbine Research: {{A}} Review of Commercial {{CFD}}, {{FE}} Codes and Experimental Practices},
  shorttitle = {Horizontal Axis Wind Turbine Research},
  author = {O'Brien, J. M. and Young, T. M. and O'Mahoney, D. C. and Griffin, P. C.},
  year = {2017},
  month = jul,
  volume = {92},
  pages = {1--24},
  issn = {0376-0421},
  doi = {10.1016/j.paerosci.2017.05.001},
  abstract = {A review of wind turbine aerodynamics research is presented. The review is limited to Horizontal Axis Wind Turbine (HAWT) investigations. The focus is on recent near wake experiments, wake predictions by commercial CFD codes and current FSI and structural modelling attempts. For the near wake, the review considers experiments carried out in controlled conditions whereby the incoming freestream is perpendicular to the rotor plane. Additional anomalies such as wind shear, gusts and yaw transition are not considered. The survey of 3D commercial codes is also focused on HAWT models in parallel flow conditions. Finally, the structural models reviewed are divided into two separate categories: 1) blade deflection and performance under aerodynamic loads, and 2) the vibrational response of blades under aerodynamic loading. The aim is to highlight common trends within near wake experiments and investigate both CFD and FE modelling strategies \textendash{} to identify current limitations and future opportunities within the sector.},
  file = {/home/san/Zotero/storage/RHGRIJZR/o'brien_et_al_2017_horizontal_axis_wind_turbine_research.pdf;/home/san/Zotero/storage/FIWM3X48/S0376042116301579.html},
  journal = {Progress in Aerospace Sciences},
  keywords = {_tablet,CFD,Fluid-structure Interaction modelling,Horizontal axis wind turbines,Near wake},
  language = {en}
}

@article{oelkerEvaluationImpactWeatherRelated2021,
  title = {Evaluation of the {{Impact}} of {{Weather}}-{{Related Limitations}} on the {{Installation}} of {{Offshore Wind Turbine Towers}}},
  author = {Oelker, Stephan and Sander, Aljoscha and Kreutz, Markus and {Ait-Alla}, Abderrahim and Freitag, Michael},
  year = {2021},
  month = jun,
  volume = {14},
  pages = {3778},
  issn = {1996-1073},
  doi = {10.3390/en14133778},
  abstract = {Weather conditions have a significant impact on the installation of offshore wind turbines. The rules for installation set clear limits. These limits are usually based on estimations of various experts and not on real assumptions and measurements on-site. When wind speeds and wave heights are too high, work cannot be carried out, and this leads to delays and additional costs. Therefore, we have carried out a measurement campaign during the installation of rotor blades to investigate to which extent the limits can be adjusted by using a tuned mass damper. The results from the measurement campaign\textemdash specifically empirically derived significant wave height limits\textemdash are used in a discrete event simulation. This study simulates delays resulting from weather conditions. Based on this, the total installation costs are considered. The results of the measurement campaign show that a safe installation with the use of a damper is possible at wave heights of up to 1.6 m. With the discrete event simulation, it is possible to prove that 17.9\% can be saved for the costs of the installation vessel. In addition, the wind farm could be erected 32 days faster. Thus, it can be stated that the use of a tuned mass damper simplifies the installation from a technical point of view and is economical.},
  copyright = {Creative Commons Attribution-ShareAlike 4.0 International Licence (CC-BY-SA)},
  file = {/home/san/Zotero/storage/LLKUTUVN/oelker_et_al_2021_evaluation_of_the_impact_of_weather-related_limitations_on_the_installation_of.pdf},
  journal = {Energies},
  language = {en},
  number = {13}
