  file = {/home/san/Zotero/storage/4NQSWPMB/2019_Panapakidis et al_Implementation of Pattern Recognition Algorithms in Processing Incomplete Wind.pdf},
  journal = {Electronics},
  keywords = {_tablet,clustering methods,europe,incomplete data,missing data,offshore wind turbines,time series   clustering,unsupervised machine learning,wave,wind speed},
