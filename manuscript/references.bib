  booktitle = {Windeurope {{Conference}} 2018 {{Within}} the {{Global Wind Summit}}},
  author = {Leimeister, M. and Balaam, T. and Causon, P. and Cevasco, D. and Richmond, M. and Kolios, A. and Brennan, F.},
