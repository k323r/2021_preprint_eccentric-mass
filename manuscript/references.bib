@incollection{segerenExperimentalInvestigationDynamic2014,
  title = {Experimental Investigation of the Dynamic Installation of a Slip Joint Connection between the Monopile and Tower of an Offshore Wind Turbine},
  booktitle = {Science of {{Making Torque}} from {{Wind}} 2014 (Torque 2014)},
  author = {Segeren, M. L. A. and Hermans, K. W.},
  editor = {Bak, C. and Bechmann, A. and Bingol, F. and Dellwik, E. and Dimitrov, N. and Giebel, G. and Hansen, M. O. L. and Jensen, D. J. and Larsen, G. and Madsen, H. A. and Mann, J. and Natarajan, A. and Rathmann, O. and Sathe, A. and Sorensen, J. N. and Sorensen, N. N.},
  year = {2014},
  volume = {524},
  pages = {012080},
  publisher = {{Iop Publishing Ltd}},
  address = {{Bristol}},
  abstract = {The failure of the traditional grouted connections of offshore wind turbines has led to the investigation of alternatives that provide a connection between the foundation pile and the turbine tower. An alternative to the traditional joint is a steel-to-steel connection also called a slip joint. To ensure a proper fit of the slip joint a dynamic installation of the joint is proposed. In this contribution, the effectiveness of harmonic excitation as an installation procedure is experimentally investigated using a 1:10 scaled model of the joint. During the dynamic installation test the applied static load, settlements and dynamic response of the joint are monitored using respectively load cells, taut wires and strain gauges placed both inside and outside the conical surfaces. The results show that settlement occurs only when applying a harmonic load at specific forcing frequencies. The settlement stabilizes to a certain level for each of the specific frequencies, indicating that a controlled way of installation is possible. The results show that it is essential to vibrate at specific frequencies and that a larger amplitude of the harmonic force does not automatically lead to additional settlement.},
  annotation = {WOS:000344193600080},
  language = {English}
}

@phdthesis{sehtsPrognoseInstallationskostenOffshoreWindenergieparks2021,
  title = {Prognose Der {{Installationskosten}} von {{Offshore}}-{{Windenergieparks}} Auf {{Basis}} von Empirischen {{Daten}}},
  author = {Sehts, Alexander},
  year = {2021},
  month = mar,
  address = {{Bremen}},
  abstract = {To make the installation of offshore wind turbines more profitable, installation costs must be minimized. This can be achieved, among other things, by reducing installation times. This work aims to establish correlations between prevailing environmental conditions and rotor blade installation to subsequently make predictions for future installation projects. Furthermore, it is investigated to what extent the use of a vibration damper changes the installation frequency in a year. For this purpose, worldwide data measurements from 2007-2017 are examined. Using the programming language "R", time windows are filtered out that have installationfriendly wind speeds and wave heights. Finally, the filtered datasets are examined for contiguous times that reflect the respective installation times required with and without vibration dampeners. It can be shown that the use of a vibration damper noticeably increases the number of possible installation windows. The probability of a successful installation is lowest in January and highest in June. Filtering wave height and wind speed reduce the number of installation windows. The elaboration offers future work the opportunity to build on the results and use them further to make sense of the analysis of past installation projects using environmental data forecasting.},
  file = {/home/san/Zotero/storage/5DX3PDDN/Bachelorarbeit Alexander_Sehts_4435584-zusammengefügt.pdf},
  keywords = {_tablet},
  school = {University of Bremen},
  type = {Bachelor of {{Science}}}
}

