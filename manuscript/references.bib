  journal = {Journal of Fluids and Structures},
  keywords = {_tablet,Four-step semi-implicit Characteristic-based split (4-SICBS),Planar shear flow,Shear rate,Two tandem cylinders,Vortex-induced vibration (VIV)},
  language = {en}
}

