@article{al-mdallalForcedStreamwiseOscillations2007a,
  title = {Forced Streamwise Oscillations of a Circular Cylinder: {{Locked}}-on Modes and Resulting Fluid Forces},
  shorttitle = {Forced Streamwise Oscillations of a Circular Cylinder},
  author = {{Al-Mdallal}, Q. M. and Lawrence, K. P. and Kocabiyik, S.},
