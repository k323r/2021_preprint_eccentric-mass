@article{jauvtisEffectTwoDegrees2004,
  title = {The Effect of Two Degrees of Freedom on Vortex-Induced Vibration at Low Mass and Damping},
  author = {Jauvtis, N. and Williamson, C. H. K.},
  year = {2004},
  month = jun,
  volume = {509},
  pages = {23--62},
  issn = {1469-7645, 0022-1120},
  doi = {10.1017/S0022112004008778},
  abstract = {Although there are a great many papers dedicated to the problem of a cylinder vibrating transverse to a fluid flow (YY-motion), there are almost no papers on the more practical case of vortex-induced vibration in two degrees of freedom (X,YX,Y motion) where the mass and natural frequencies are precisely the same in both XX- and YY-directions. We have designed the present pendulum apparatus to achieve both of these criteria. Even down to the low mass ratios, where m{${_\ast}$}=6m\^*\textbackslash,\{=\}\textbackslash,6, it is remarkable that the freedom to oscillate in-line with the flow affects the transverse vibration surprisingly little. The same response branches, peak amplitudes, and vortex wake modes are found for both YY-only and X,YX,Y motion. There is, however, a dramatic change in the fluid\textendash structure interactions when mass ratios are reduced below m{${_\ast}$}=6m\^*\textbackslash,\{=\}\textbackslash,6. A new amplitude response branch with significant streamwise motion appears, in what we call the `super-upper' branch, yielding massive amplitudes of 3 diameters peak-to-peak (AY{${_\ast}\sim$}1.5A\^*\_Y \textbackslash,\{\textbackslash sim\}\textbackslash, 1.5). We discover a corresponding periodic vortex wake mode, comprising a triplet of vortices being formed in each half-cycle, in what we define as a `2T' mode. We qualitatively interpret the principal vortex dynamics and vortex forces which yield a positive rate of energy transfer (e\textperiodcentered V\textbackslash dot\{e\}\_V) causing the body vibration, using the following simple equation: e\textperiodcentered V=2{$\Gamma{_\ast}$}UV{${_\ast}$}Y\textperiodcentered\textbackslash dot\{e\}\_V = 2 \{\textbackslash Gamma\}\^* U\^*\_V \textbackslash skew3\textbackslash dot\{Y\} where {$\Gamma{_\ast}\backslash$}Gamma\^* is vortex strength, UV{${_\ast}$}U\^*\_V is the speed downstream of the dominant near-wake vorticity, and Y\textperiodcentered\textbackslash skew3\textbackslash dot\{Y\} is the transverse velocity of the body. This simple approach suggests that the massive amplitude of vibration for the 2T mode is principally attributed to the energy transfer from the `third' vortex of each triplet, which is not present in the lower-amplitude 2P mode. We also find two low-speed streamwise vibration modes, which is not unexpected, since they correspond to the first and second excitation modes of vibration for flexible cantilevers. By considering equations of motion for the two degrees of freedom, we find a critical mass, m\textbackslash scriptsize\textbackslash it crit{${_\ast}$}=0.52m\^*\_\{\textbackslash hbox\{\textbackslash scriptsize\textbackslash it crit\}\} \textbackslash,\{=\}\textbackslash, 0.52, similar to recent YY-only studies, below which the large-amplitude vibrations persist to infinite flow velocity. We show that the critical mass m\textbackslash scriptsize\textbackslash it crit{${_\ast}$}m\^*\_\{\textbackslash hbox\{\textbackslash scriptsize\textbackslash it crit\}\} is the same for the XX- and YY-directions, which ensures that the shapes of X,YX, Y trajectories can retain their form as the velocity becomes large. The extensive studies of vortex-induced vibration for YY-only body motions, built up over the last 35 years, remain of strong relevance to the case of two degrees of freedom, for m{${_\ast}>$}6m\^* \textbackslash,\{{$>\rbrace\backslash$}, 6. It is only for `small' mass ratios, m{${_\ast}<$}6m\^* \textbackslash,\{{$<\rbrace\backslash$}, 6, that one observes a rather dramatic departure from previous results, which would suggest a possible modification to offshore design codes.\vphantom{\}\}}},
  file = {/home/san/Zotero/storage/HTARS3DS/Jauvtis_Williamson_2004_The effect of two degrees of freedom on vortex-induced vibration at low mass.pdf;/home/san/Zotero/storage/BN7S52QN/C5BF4BED38FF90EC11DD3CF8D862EDD7.html;/home/san/Zotero/storage/G2292UU4/C5BF4BED38FF90EC11DD3CF8D862EDD7.html},
  journal = {Journal of Fluid Mechanics},
  keywords = {_tablet},
  language = {en}
}

