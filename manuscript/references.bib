@article{parkInvestigationImpactsPassive2019,
  title = {An Investigation on the Impacts of Passive and Semiactive Structural Control on a Fixed Bottom and a Floating Offshore Wind Turbine},
  author = {Park, Semyung and Lackner, Matthew A. and Pourazarm, Pariya and Rodr{\'i}guez Tsouroukdissian, Arturo and Cross-Whiter, John},
