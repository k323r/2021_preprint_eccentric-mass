@article{pageImpactFoundationModelling2019,
  ids = {pageImpactFoundationModelling2019a},
  title = {Impact of Foundation Modelling in Offshore Wind Turbines: {{Comparison}} between Simulations and Field Data},
  shorttitle = {Impact of Foundation Modelling in Offshore Wind Turbines},
  author = {Page, Ana M. and N{\ae}ss, Veronika and De Vaal, Jacobus B. and Eiksund, Gudmund R. and Nygaard, Tor Anders},
  year = {2019},
  month = mar,
  volume = {64},
  pages = {379--400},
  issn = {09518339},
  doi = {10.1016/j.marstruc.2018.11.010},
  abstract = {The design of Offshore Wind Turbines (OWTs) relies on integrated simulation tools capable of predicting the system dynamic characteristics and the coupled loads and responses. Despite all efforts to develop accurate integrated models, these often fail to reproduce the measured natural frequencies, partly due to the modelling of the foundation. Several foundation models and calibration approaches have been proposed and compared with small or large scale field tests, where only the soil and the foundation are included. However, there is a lack of more integral validation where the interaction between the foundation and the structure is taken into account. The paper investigates the impact of the foundation model and calibration approach on the simulated response of a monopile-based OWT installed in the North Sea by comparing simulations and full-scale field data. The OWT structure and the environmental actions are implemented in the aero-servo-hydro-elastic code 3DFloat. Two foundation models and two calibration approaches are evaluated. The results indicate that, with a conceptually correct foundation model and a realistic calibration, it is possible to match the measured natural frequency and predict accurate fatigue loads. More accurate predicted loads will reduce uncertainties in the estimated fatigue lifetime and therefore reduce risk in the design.},
  file = {/home/san/Zotero/storage/CAWCANWE/page_et_al_2019_impact_of_foundation_modelling_in_offshore_wind_turbines.pdf;/home/san/Zotero/storage/L5APV6WJ/page_et_al_2019_impact_of_foundation_modelling_in_offshore_wind_turbines.pdf},
  journal = {Marine Structures},
  keywords = {_tablet},
  language = {en}
}

