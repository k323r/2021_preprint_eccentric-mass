@article{westGlobalPatternsOffshore2021,
  title = {Global Patterns of Offshore Wind Variability},
  author = {West, Chris G. and Smith, Ronald B.},
  year = {2021},
  month = mar,
  volume = {n/a},
  issn = {1099-1824},
  doi = {10.1002/we.2641},
  abstract = {Using the 40-year hourly gridded ERA5 reanalysis, we study the offshore patterns of wind variability using the probability density function (PDF) and the power spectral density (PSD). To summarize wind variability, we compute the Weibull parameters from the PDF and the PSD for six spectral bands: interannual, annual, multimonth, storm, diurnal, and semidiurnal. We characterize the storm spectral peak using a Gaussian function in frequency space. These parameters are plotted along two pole-to-pole transects through the Atlantic Ocean at 25\textdegree W and the Pacific Ocean at 170\textdegree W longitude and at 16 current and proposed offshore wind sites. We test the idea that coastal and open-ocean wind statistics may be described by a single set of meridional profiles. In mid-latitudes, the storm band variance exceeding 20 m2 s-2 dominates. The Weibull shape parameter is close to . The storm interval varies from 4 to 7 days depending on location. In the Northern Hemisphere only, the annual variance is significant, exceeding 3 m2 s-2. In the equatorial region, we find steadier winds with small storm variance less than 3 m2 s-2. The Weibull shape parameter is or greater, suggesting the possibility of a high capacity factor there. The storm interval can exceed 15 days. The equatorial zone has an annual wind variance reaching 2 m2 s-2, comparable to the storm variance. In the tropical Pacific, we find interannual variance associated with El Ni\~no-Southern Oscillation (ENSO). Semidiurnal variance is detected in the tropics from the atmospheric tide.},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/we.2641},
  file = {/home/san/Zotero/storage/GXCH3WWU/west_smith_global_patterns_of_offshore_wind_variability.pdf;/home/san/Zotero/storage/PW9S9CR8/we.html},
